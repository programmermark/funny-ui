{
  "name": "@funny-ui/ui",
  "version": "0.0.0",
  "description": "A components library based on Vue3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "types/index.d.ts",
  "vetur": {
    "tags": "highlight/tags.json",
    "attributes": "highlight/attributes.json"
  },
  "web-types": "highlight/web-types.json",
  "keywords": [
    "Vue",
    "UI"
  ],
  "author": "zhonghui <1126765590hui@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/programmermark/funny-ui.git"
  },
  "bugs": {
    "url": "https://github.com/programmermark/funny-ui/issues"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "es/style.js",
    "es/less.js",
    "lib/style.js",
    "lib/less.js",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "varlet-cli dev",
    "build": "varlet-cli build",
    "preview": "varlet-cli preview",
    "compile": "varlet-cli compile",
    "lint": "varlet-cli lint",
    "changelog": "varlet-cli changelog",
    "release": "pnpm compile && varlet-cli release",
    "test": "varlet-cli jest",
    "test:watch": "varlet-cli jest -w",
    "test:watchAll": "varlet-cli jest -wa",
    "create": "varlet-cli create",
    "deploy:docs": "node deploy/deploy-docs.js"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@varlet/cli": "^2.0.0",
    "@varlet/eslint-config": "^2.0.0",
    "@varlet/icons": "^2.0.0",
    "@varlet/shared": "^2.0.0",
    "@varlet/stylelint-config": "^2.0.0",
    "@varlet/touch-emulator": "^2.0.0",
    "@vue/test-utils": "^2.0.2",
    "clipboard": "^2.0.6",
    "eslint": "^7.30.0",
    "lint-staged": "^10.5.0",
    "live-server": "^1.2.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.4",
    "vue": "3.2.25",
    "vue-router": "4.0.12"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,less}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix",
    "*.{vue,css,less}": "stylelint --fix"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "lib/**",
      "es/**",
      "umd/**",
      "site/**",
      "public/**",
      "src/*/__tests__/**",
      "coverage/**",
      "highlight/**",
      "types/index.d.ts",
      ".varlet/**"
    ],
    "extends": [
      "@varlet"
    ]
  },
  "stylelint": {
    "extends": [
      "@varlet/stylelint-config"
    ],
    "ignoreFiles": [
      "lib/**",
      "es/**",
      "umd/**",
      "site/**",
      "coverage/**",
      "public/**",
      "highlight/**",
      ".varlet/**"
    ]
  },
  "browserslist": [
    "Chrome >= 54",
    "iOS >= 10"
  ],
  "packageManager": "pnpm@7.9.0",
  "files": [
    "es",
    "lib",
    "umd",
    "highlight",
    "types"
  ],
  "dependencies": {
    "@funny-ui/icons": "^0.0.1",
    "@vue/shared": "^3.2.41",
    "@vueuse/core": "^9.3.1",
    "lodash-es": "^4.17.21"
  }
}